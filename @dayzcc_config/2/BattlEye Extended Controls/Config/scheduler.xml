<?xml version="1.0"?>
<!-- Written by Crosire -->
<Scheduler>
	
    <!-- Load Bans -->
    <job id="0">
	<time>000100</time>
	<day>1,2,3,4,5,6,7</day>
	<loop>1</loop>
	<cmd>loadbans</cmd>
	<cmdtype>0</cmdtype>
    </job>

    <!-- Restart (Every 6 hours) -->
    <job id="1">
	<time>055500</time>
	<day>1,2,3,4,5,6,7</day>
	<loop>1</loop> 
	<cmd>say -1 Server will reload the mission in 5 minutes</cmd>    
	<cmdtype>0</cmdtype>            
    </job>
	
    <job id="2">
	<time>055900</time>
	<day>1,2,3,4,5,6,7</day> 
	<loop>1</loop> 
	<cmd>say -1 Server will reload the mission in 1 minute</cmd>    
	<cmdtype>0</cmdtype>            
    </job>

    <job id="3">
	<time>060000</time>
	<day>1,2,3,4,5,6,7</day>
	<loop>1</loop> 
	<cmd>#restart</cmd>    
	<cmdtype>0</cmdtype>            
    </job>

</Scheduler>