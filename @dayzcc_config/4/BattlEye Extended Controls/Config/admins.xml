<?xml version="1.0"?>
<BEAdmins>

	<!-- 
	define admins.
	
	name : name of the admin. do not need to be ingame player name.
	guid : match players guid.
	group : admin level. value must be a number.
	groupname : name of the group the admin has.
	
	Note: keep <..id= > in order for your admin block. else parsing the file will cause errors.
	-->
	
	<admin id="0">
		<name></name>
		<guid></guid>
		<group>0</group>
		<groupname>Super Admin</groupname>
	</admin>
	
</BEAdmins>
